	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"browserslist": [
		"last 2 versions",
		"> 1%"
	],
	"keywords": [],
	"author": "",
	"license": "ISC",
